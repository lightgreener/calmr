<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name,:autofocus => true %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>

  <%= f.label :gender %>
  <%= f.text_field :gender %>

  <%= f.label :age %>
  <%= f.number_field :age %>

  <%= f.label :image %>
  <%= f.url_field :image %>

  <%= f.submit %>
<% end %>
